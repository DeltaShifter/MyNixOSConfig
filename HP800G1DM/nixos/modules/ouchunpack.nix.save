uot;,null,1,null,null,1,0]];mutable:true"><span class="mat-mdc-button-persistent-ripple mdc-icon-button__ripple"></span><mat-icon _ngcontent-ng-c2705694554="" role="img" fonticon="content_copy" class="mat-icon notranslate gds-icon-s google-symbols mat-ligature-font mat-icon-no-color" aria-hidden="true" data-mat-icon-type="font" data-mat-icon-name="content_copy"></mat-icon><span class="mat-focus-indicator"></span><span class="mat-mdc-button-touch-target"></span></button><!----><!----></div><!----></div><!----><div _ngcontent-ng-c2705694554="" class="formatted-code-block-internal-container ng-tns-c2705694554-62"><div _ngcontent-ng-c2705694554="" class="animated-opacity ng-tns-c2705694554-62"><pre _ngcontent-ng-c2705694554="" class="ng-tns-c2705694554-62"><code _ngcontent-ng-c2705694554="" role="text" data-test-id="code-content" class="code-container formatted ng-tns-c2705694554-62">sudo -E gparted
